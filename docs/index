<link role="panic-overlay-devonly" rel="stylesheet" href="/js/panic-overlay.browser.js.css" /><script role="panic-overlay-devonly" src="/js/panic-overlay.browser.js"></script><html><head><script role="runtime" src="/runtime/ltv.js" type="text/javascript"></script><script>const { render } = Vanil;
// browser language detection (we only want lang)
// see https://developer.mozilla.org/en-US/docs/Web/API/Navigator/language
const lang = (navigator.language || navigator.userLanguage).split('-')[0];
// use web standard redirect via <meta> refresh
render(Vanil.tsx("meta", { "http-equiv": "refresh", content: `0; url=/${lang}` }), document.head);
</script></head><body><noscript>Please chose a language:<a href="/de">German</a><a href="/en">English</a></noscript></body></html>