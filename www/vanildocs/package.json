{
  "name": "vanildocs",
  "version": "0.0.1",
  "description": "VanilDocs generated using: npx vanil init",
  "scripts": {
    "dev": "vanil dev",
    "dev:head": "npm --prefix ../../ run start:dev -- dev --project-root ./www/vanildocs",
    "build:head": "npm --prefix ../../ run start:dev -- build --project-root ./www/vanildocs",
    "preview:head": "npm --prefix ../../ run start:dev -- preview --project-root ./www/vanildocs",
    "preview": "vanil preview",
    "config": "vanil config",
    "clean": "vanil clean"
  },
  "author": "Aron Homberg <info@aron-homberg>",
  "license": "MIT",
  "browserslist": [
    "> 0.05%",
    "not ie > 0"
  ],
  "vanil": {
    "dist": "../../docs",
    "devOptions": {
      "useOptimizer": false
    }
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "sass": "^1.44.0",
    "vanil": "^0.0.51"
  },
  "devDependencies": {
    "@types/sass": "^1.43.1"
  }
}